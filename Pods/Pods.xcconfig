GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/CocoaAsyncSocket" "${PODS_ROOT}/Headers/CocoaLumberjack" "${PODS_ROOT}/Headers/FMDB" "${PODS_ROOT}/Headers/HPLChatViewController" "${PODS_ROOT}/Headers/XMPPFramework" $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libresolv
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/XMPPFramework/Vendor/libidn" "$(PODS_ROOT)/XMPPFramework/Vendor/libidn"
OTHER_CFLAGS = $(inherited) "-isystem${PODS_ROOT}/Headers" "-isystem${PODS_ROOT}/Headers/CocoaAsyncSocket" "-isystem${PODS_ROOT}/Headers/CocoaLumberjack" "-isystem${PODS_ROOT}/Headers/FMDB" "-isystem${PODS_ROOT}/Headers/HPLChatViewController" "-isystem${PODS_ROOT}/Headers/XMPPFramework"
OTHER_LDFLAGS = -ObjC -lidn -lresolv -lsqlite3 -lxml2 -framework CFNetwork -framework CoreData -framework CoreLocation -framework Security -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/Pods